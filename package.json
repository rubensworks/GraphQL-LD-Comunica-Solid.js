{
  "name": "graphql-ld-comunica-solid",
  "version": "2.0.0",
  "description": "A GraphQL-LD engine that is backed by Comunica with Solid authentication",
  "keywords": [
    "graphql",
    "graphqlld",
    "sparql",
    "solid",
    "query",
    "rdf",
    "linked data",
    "comunica"
  ],
  "main": "index.js",
  "typings": "index",
  "repository": "git@github.com:rubensworks/graphql-ld-comunica-solid.js.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/graphql-ld-comunica-solid.js/issues"
  },
  "homepage": "https://github.com/rubensworks/graphql-ld-comunica-solid.js#readme",
  "license": "MIT",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "jest": "^26.0.0",
    "manual-git-changelog": "^1.0.1",
    "pre-commit": "^1.2.2",
    "streamify-string": "^1.0.1",
    "ts-jest": "^26.0.0",
    "tslint": "^6.0.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest ${1}",
    "test-watch": "jest ${1} --watch",
    "lint": "tslint index.ts lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc",
    "validate": "npm ls",
    "prepare": "npm run build",
    "version": "manual-git-changelog onversion"
  },
  "dependencies": {
    "@comunica/actor-init-sparql-solid": "^1.0.0",
    "graphql-ld": "^1.4.0",
    "sparqlalgebrajs": "^4.0.0"
  },
  "resolutions": {
    "@types/yargs": "17.0.4"
  }
}
